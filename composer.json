{
    "name": "fansipan/fansipan",
    "description": "A simple package that allows you to write your API integrations or SDKs in a elegant way.",
    "keywords": [
        "http",
        "sdk",
        "api",
        "psr7",
        "psr17",
        "psr18"
    ],
    "homepage": "https://github.com/phanxipang/fansipan",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "jenky.w0w@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "php-http/discovery": "^1.14",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "guzzlehttp/guzzle": "^7.5",
        "fansipan/mock-client": "^1.0",
        "nyholm/psr7": "^1.5",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "provide": {
        "psr/http-client-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Fansipan\\": "src"
        },
        "classmap": [
            "stubs"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fansipan\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse"
        ],
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage"
        ],
        "check": [
            "@cs --dry-run",
            "@analyse",
            "@test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
